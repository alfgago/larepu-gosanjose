{
    "name": "custom-spatie/statamic-responsive-images",
    "license": "MIT",
    "type": "statamic-addon",
    "description": "Responsive Images for Statamic 3",
    "autoload": {
        "psr-4": {
            "Spatie\\ResponsiveImages\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Spatie\\ResponsiveImages\\Tests\\": "tests"
        }
    },
    "extra": {
        "statamic": {
            "name": "Responsive Images",
            "description": "Responsive Images for Statamic 3"
        },
        "laravel": {
            "providers": [
                "Spatie\\ResponsiveImages\\ServiceProvider"
            ]
        }
    },
    "require": {
        "statamic/cms": "~3.3"
    },
    "require-dev": {
        "orchestra/testbench": "^6.9|^7.1",
        "phpunit/phpunit": "^9.5",
        "spatie/phpunit-snapshot-assertions": "^4.2"
    },
    "config": {
        "allow-plugins": {
            "pixelfear/composer-dist-plugin": true
        }
    }
}
